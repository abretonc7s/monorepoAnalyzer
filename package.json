{
  "name": "monorepo-analyzer",
  "version": "1.0.0",
  "main": "src/monorepoAnalyser.ts",
  "license": "MIT",
  "packageManager": "yarn@4.5.1",
  "scripts": {
    "start": "ts-node src/monorepoAnalyser.ts",
    "lint": "eslint --fix .",
    "size": "size-limit",
    "build": "rollup -c",
    "analyze-all": "ts-node src/tools/optimization/cli.ts analyze",
    "clean": "rm -rf dist stats optimization-report.html",
    "predeploy": "yarn analyze-all",
    "deploy": "gh-pages -d . -f --src '{index.html,optimization-report.html,stats/**/*,dist/**/*}'",
    "serve": "serve .",
    "preview": "yarn analyze-all && serve ."
  },
  "dependencies": {
    "@coinbase/wallet-sdk": "^4.2.2",
    "@metamask/sdk": "^0.30.2",
    "@types/node": "^22.7.9",
    "@walletconnect/web3wallet": "^1.16.1",
    "chalk": "^4.1.2",
    "fs-extra": "^11.2.0",
    "glob": "^11.0.0",
    "madge": "^8.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@eslint/js": "^9.13.0",
    "@rollup/plugin-babel": "^6.0.0",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@size-limit/preset-app": "^11.1.6",
    "@size-limit/webpack": "^11.1.6",
    "@types/fs-extra": "^11.0.4",
    "@types/gh-pages": "^6",
    "@types/madge": "^5.0.3",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "babel-loader": "^9.1.3",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "gh-pages": "^6.2.0",
    "globals": "^15.11.0",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rollup": "^4.25.0",
    "rollup-plugin-visualizer": "^5.0.0",
    "serve": "^14.2.1",
    "size-limit": "^11.1.6",
    "typescript-eslint": "^8.11.0"
  }
}
